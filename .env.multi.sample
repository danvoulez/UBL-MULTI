# Storage
CAS_BACKEND=fs
CAS_FS_DIR=./data/cas
LEDGER_DIR=./data/ledger
INDEX_DIR=./data/index
EVENTSTORE_DIR=./data/eventstore

# Canon/Numeric strictness
REQUIRE_UNC1_NUMERIC=true
F64_IMPORT_MODE=reject

# Extension defaults for third-party platform mode
UBL_FEATURE_AUDIT_ENABLED=true
UBL_FEATURE_ADVISOR_ENABLED=true
UBL_FEATURE_MCP_ENABLED=true

# Event / durability
UBL_EVENTSTORE_ENABLED=true
UBL_EVENTSTORE_PATH=./data/events
UBL_STORE_BACKEND=sqlite
UBL_STORE_DSN=file:./data/ubl.db?mode=rwc&_journal_mode=WAL
UBL_IDEMPOTENCY_BACKEND=sqlite
UBL_IDEMPOTENCY_DSN=file:./data/ubl.db?mode=rwc&_journal_mode=WAL
UBL_OUTBOX_BACKEND=sqlite
UBL_OUTBOX_DSN=file:./data/ubl.db?mode=rwc&_journal_mode=WAL
UBL_OUTBOX_WORKERS=2

# Identity / keys
REGISTRY_BASE_URL=http://127.0.0.1:4000
UBL_DIDKEY_FORMAT=compat
KMS_SEED=
